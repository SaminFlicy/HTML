<script>
function findSum() {
var i;
var max=0;
for(i = 0; i < arguments.length; i++) {
  
         
      max =arguments[i] + max ;


}
return max; 

}
document.getElementById("result").innerHTML = findSum(45,6,7,23,86,9)









