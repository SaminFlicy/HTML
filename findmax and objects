<script>
function findMax() {
var i;
var max=0;
for(i = 0; i < arguments.length; i++) {
    if (arguments[i] > max){

         
      max =arguments[i] ;

}
}
return max; 

}
document.getElementById("result").innerHTML = findMax(534535445645,787834874,9,000,78278333333333333333333333333333333333333333,6,7,999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999,777777777777777777777777777777777777777777777777777777777777777)









</script>

