    <p> dont click the button too get a time based greetings lol its a scam </p>
 <button onclick="myFunction()">SCAM</button>
 <p id="hour"></p>
 <p id="greet"></p>
<script>
       function myFunction() {
          var greeting;
          var time = new Date().getHours() 


          if (time < 10) {
           greeting = "Good morning"
          }

          else if (time < 20) {
           greeting = "Good Day"
          }

          else {
           greeting = "GOOD EVENING"
          }

  
         document.getElementById("hour").innerHTML = "Current Hour is:"
         + time;
         document.getElementById("greet").innerHTML = greeting;


}

















</script>







