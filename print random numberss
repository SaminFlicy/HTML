    <p> ur marks here</p>
 <button onclick="myFunction()">YOUR NUMBER</button>
 <p id="marks"></p>
 <p id="greet"></p>
<script>
       function myFunction() {
          var greeting;
          var time = new Date().getHours() 


          if (time < 67) {
           greeting = "Work Harder"
          }

          else if (time < 87) {
           greeting = "Good"
          }

          else {
           greeting = "V.GOOD"
          }

  
         document.getElementById("marks").innerHTML = "YOUR:"
         + time;
         document.getElementById("greet").innerHTML = greeting;


}

















</script>







</body>
</html>
